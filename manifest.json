{
	"name": "History Disabler",
	"version": "1.4",
	"manifest_version": 2,
	"description": "Disable history in Chrome.",
	"background": {
		"scripts": ["/js/background.js"],
		"persistent": false
	},
	"icons": {
		"128": "/img/icon128.png"
	},
	"permissions": [
		"browsingData",
		"history",
		"tabs"
	]
}